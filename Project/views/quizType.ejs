    <link rel='stylesheet' href='http://codepen.io/assets/libs/fullpage/jquery-ui.css'>

    <link rel="stylesheet" href="../stylesheets/style1.css" media="screen" type="text/css" />
    <!--<link rel="stylesheet" type="text/css" href="./quiz - Question 1_files/css">-->
    <!--<link rel="stylesheet" type="text/css" href="./quiz - Question 1_files/css(1)">-->
    <!--<link rel="stylesheet" type="text/css" href="./quiz - Question 1_files/css(2)">-->
    <link rel="shortcut icon" href="http://www.onlinequizcreator.com/favicon.ico">
    <link rel="stylesheet" type="text/css" href="../stylesheets/A.styles.css.pagespeed.cf.xV6htyUXH4.css">

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css" media="all" />
    <script src='/javascripts/jquery-1.8.1.min.js' type='text/javascript'></script>
    <script src='/javascripts/jquery.kwicks.js' type='text/javascript'></script>
    <script type='text/javascript'>
        $(function() {
            $('.kwicks').kwicks({
                maxSize : 250,
                spacing : 5,
                behavior: 'menu'
            });
        });
    </script>


</head>
<body class="bg">
<div class="header-bg">
    <div class="wrap">
        <div class="header">
            <div class="logo">
                <a href="/"><img src="/images/logoquiz1.jpg"> </a>
            </div>
            <div class="menu">
                <% include menu_header %>
            </div>
            <div class="clear"></div>
        </div>
    </div>
</div>
<div class="navbar">
    <div class="wrapper clearfix">
        <nav class="langswitch">
        </nav>


        <div class="pageheader" style="margin-top: 50px;">

            <div class="container">

                <div class="row clearfix introbar" id="startedit">
                    <div class="col12">
                        <div class="contentbox createquiz">
                            <h2>Lets Create the Quiz !!!</h2></br>

                            <form id="quizType" action="/quizType/quiz" method="POST">
                                <fieldset>
                                    <div>
                                        <input placeholder="Enter the name of the Quiz" id="quizname" name="quizname" type="text" required="" tabindex="1"></div>
                                </fieldset>
                                <fieldset class="createanswers">
                                    <h2>Select Quiz Type</h2>
                                    <div>
                                        <select class="select-style gender" name="quizType">
                                        <!--<option value="select">I am ...</option>-->
                                        <option value="Economics">Economics</option>
                                        <option value="Computer">Computer</option>
                                        <option value="Science">Science</option>
                                        </select><br><br>

                                        <!--<input placeholder="Answer A" id="AnswerA" name="AnswerA" type="text" required="" tabindex="2"></div>-->

                                </fieldset>
                                <div class="row prevnext"><input class="new" name="Next" type="submit" value="Next">		</div>



                            </form>
                        </div>
                    </div>
                </div>
            </div>


        </div>


    </div>
</div>
<% include footer %>