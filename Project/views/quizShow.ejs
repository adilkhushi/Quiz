<!DOCTYPE HTML>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <title>Quiz Challenger</title>
    <meta charset="UTF-8">



    <link rel='stylesheet' href='http://codepen.io/assets/libs/fullpage/jquery-ui.css'>

    <link rel="stylesheet" href="../stylesheets/style1.css" media="screen" type="text/css" />
    <link rel="shortcut icon" href="http://www.onlinequizcreator.com/favicon.ico">
    <link rel="stylesheet" type="text/css" href="../stylesheets/A.styles.css.pagespeed.cf.xV6htyUXH4.css">

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
    <link href="/stylesheets/style.css" rel="stylesheet" type="text/css" media="all" />
    <script src='/javascripts/jquery-1.8.1.min.js' type='text/javascript'></script>
    <script src='/javascripts/jquery.kwicks.js' type='text/javascript'></script>
    <script type='text/javascript'>
        $(function() {
            $('.kwicks').kwicks({
                maxSize : 250,
                spacing : 5,
                behavior: 'menu'
            });
        });
    </script>


    <script>
        $(document).ready(function(){
            $('#quizCreate').click(function(){
                //alert("ok");
                $('#quizIndex').attr('action', '/quizType');
            });


            $('#quizShow').click(function(){
                $('#quizIndex').attr('action', '/page2');
            });


        });

    </script>

</head>
<body class="bg">
<div class="header-bg">
    <div class="wrap">
        <div class="header">
            <div class="logo">
                <a href="/"><img src="/images/logoquiz1.jpg"> </a>
            </div>
            <div class="menu">
                <% include menu_header %>
            </div>
            <div class="clear"></div>
        </div>
    </div>
</div>
<div class="navbar">
    <div class="wrapper clearfix">
        <nav class="langswitch">
        </nav>


        <div class="pageheader" style="margin-top: 50px;">

            <div class="container">

                <div class="row clearfix introbar" id="startedit">
                    <div class="col12">
                        <div class="contentbox createquiz">
                            <h2>Welcome to Quiz Menu !!!</h2></br>

                            <form id="quizIndex" method="get">
                                <fieldset>
                                    <% quizList.forEach(function(quizList) { %>
                                   <span> <a href="http://localhost:3000/quizsolve?qid=<%= quizList._id %>" style=color:cornflowerblue><%= quizList.QuizName %></a></span><br />
                                    <% }); %>
                                </fieldset>



                            </form>
                        </div>
                    </div>
                </div>
            </div>


        </div>


    </div>
</div>
<% include footer %>